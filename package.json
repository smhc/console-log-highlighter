{
  "name": "epmtestharness-runner",
  "displayName": "epmtestharness runner",
  "description": "Run individual epmtestharness tests",
  "version": "0.0.1",
  "publisher": "smhc",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:epm"
  ],
  "keywords": ["epm", "epmtestharness", "test"],
  "main": "./out/extension",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
